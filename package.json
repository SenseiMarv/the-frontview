{
  "name": "the-frontview",
  "author": "Marvin Stickel <marv.stickel@gmail.com>",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "astro dev",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "yarn eslint src/",
    "lint:prettier": "yarn prettier --list-different .",
    "lint:types": "tsc",
    "fixlint": "npm-run-all --parallel fix:*",
    "fix:eslint": "yarn eslint src/ --fix;",
    "fix:prettier": "yarn prettier --write .",
    "build": "tsc && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/inter": "^4.5.12",
    "@fontsource/quicksand": "^4.5.10",
    "@fontsource/source-code-pro": "^4.5.12",
    "astro": "^1.2.3",
    "astro-icon": "^0.7.3",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@astrojs/image": "^0.7.0",
    "@astrojs/mdx": "^0.11.1",
    "@astrojs/partytown": "^1.0.0",
    "@astrojs/prefetch": "^0.0.7",
    "@astrojs/react": "^1.1.2",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^1.0.1",
    "@astrojs/vercel": "^2.0.0",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "astro-compress": "^1.0.8",
    "astro-eslint-parser": "^0.6.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-astro": "^0.19.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mdx": "^2.0.4",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
