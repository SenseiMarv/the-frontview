{
  "name": "the-frontview",
  "author": "Marvin Stickel <marv.stickel@gmail.com>",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "astro dev",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "yarn eslint src/",
    "lint:prettier": "yarn prettier --list-different .",
    "lint:stylelint": "yarn stylelint \"**/*.css\"",
    "lint:types": "tsc",
    "fixlint": "npm-run-all --parallel fix:*",
    "fix:eslint": "yarn eslint src/ --fix;",
    "fix:prettier": "yarn prettier --write .",
    "fix:stylelint": "yarn stylelint \"**/*.css\" --fix",
    "build": "astro check && tsc && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install"
  },
  "dependencies": {
    "astro": "^1.1.3"
  },
  "devDependencies": {
    "@astrojs/image": "^0.5.1",
    "@astrojs/mdx": "^0.11.1",
    "@astrojs/partytown": "^1.0.0",
    "@astrojs/prefetch": "^0.0.7",
    "@astrojs/react": "^1.1.1",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^1.0.0",
    "@astrojs/vercel": "^1.0.2",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "astro-compress": "^1.0.7",
    "astro-eslint-parser": "^0.6.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-astro": "^0.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.4",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^28.0.0"
  },
  "lint-staged": {
    "*.{astro,js,ts,tsx}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
