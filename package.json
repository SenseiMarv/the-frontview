{
  "name": "the-frontview",
  "author": "Marvin Stickel <marv.stickel@gmail.com>",
  "license": "MIT",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "astro dev",
    "lint": "concurrently -c 'auto' 'npm:lint:*'",
    "lint:eslint": "pnpm eslint src/",
    "lint:prettier": "pnpm prettier --list-different --plugin-search-dir=. .",
    "lint:types": "tsc",
    "lint:astro": "astro check",
    "fixlint": "concurrently -c 'auto' 'npm:fix:*'",
    "fix:eslint": "pnpm eslint src/ --fix;",
    "fix:prettier": "pnpm prettier --write --plugin-search-dir=. .",
    "build": "astro check && tsc && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/rss": "1.0.3",
    "@fontsource/inter": "4.5.14",
    "@fontsource/quicksand": "4.5.11",
    "@fontsource/source-code-pro": "4.5.12",
    "astro": "1.6.12",
    "astro-embed": "0.2.0",
    "date-fns": "2.29.3",
    "hastscript": "7.1.0",
    "mdast-util-to-string": "3.1.0",
    "reading-time": "2.0.0-1",
    "rehype-add-classes": "1.0.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-wrap": "1.1.0",
    "remark-hint": "1.0.10",
    "remark-shiki-twoslash": "3.1.0",
    "remark-toc": "8.0.1",
    "sharp": "0.31.2",
    "theme-change": "2.2.0"
  },
  "devDependencies": {
    "@astrojs/image": "0.12.0",
    "@astrojs/mdx": "0.12.0",
    "@astrojs/partytown": "1.0.2",
    "@astrojs/prefetch": "0.1.1",
    "@astrojs/sitemap": "1.0.0",
    "@astrojs/tailwind": "2.1.3",
    "@astrojs/vercel": "2.3.4",
    "@tailwindcss/line-clamp": "0.4.2",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "astro-compress": "1.1.15",
    "astro-eslint-parser": "0.9.0",
    "astro-robots-txt": "0.3.10",
    "astro-seo": "0.6.0",
    "autoprefixer": "10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-astro": "0.21.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-mdx": "2.0.5",
    "eslint-plugin-n": "15.5.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tailwindcss": "3.7.1",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "postcss": "8.4.19",
    "prettier": "2.8.0",
    "prettier-plugin-astro": "0.7.0",
    "prettier-plugin-tailwindcss": "0.2.0",
    "rollup": "3.5.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.3"
  },
  "engines": {
    "node": ">=16.0.0 <18.0.0",
    "pnpm": ">=7.0.0"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "**/*": "prettier --write --plugin-search-dir=. --ignore-unknown"
  }
}
